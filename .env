# Exemple de configuration pour les scripts (copie ce fichier en .env et modifie les valeurs)
export PROJECT_ID=pagerank-2
export BUCKET=page-rank-ce
export CLUSTER_NAME=dataproc-diagramme
export REGION=europe-west1
export ZONE=      # leave empty to let Dataproc pick an auto-zone for the region
export SINGLE_NODE=false
export IMAGE_VERSION=2.1-debian11
# Security options
# NO_EXTERNAL_IP=true will create nodes without external IPs (recommended for private clusters)
export NO_EXTERNAL_IP=false
# SUBNET can set a VPC subnetwork: projects/PROJECT/regions/REGION/subnetworks/SUBNET
export SUBNET=

export MACHINE_TYPE=n1-standard-4

export MASTER_BOOT_DISK_SIZE=200
export WORKER_BOOT_DISK_SIZE=200

MACHINE_TYPE=n1-standard-4

export MASTER_BOOT_DISK_SIZE=200
export WORKER_BOOT_DISK_SIZE=200

JOB_LOCAL_PATH=diagramme.py

export GCS_INPUT=gs://${BUCKET}/data/wikilinks_lang=en_1pct.ttl

